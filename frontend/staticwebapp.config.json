{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/static/*", "/*.css", "/*.js"]
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' *.ngrok.io; img-src 'self' blob: data:; connect-src 'self' *.ngrok.io; frame-ancestors 'none'",
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  }
} 